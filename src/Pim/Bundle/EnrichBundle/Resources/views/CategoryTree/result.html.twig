{% extends 'PimSearchBundle:Search:baseResultItem.html.twig' %}

{% set showImage = true %}
{% set imagePath = asset('/bundles/pimui/images/icon_category.png') %}

{% if entity %}
    {% set recordUrl = indexer_item.recordUrl %}
{% endif %}

{% set title = entity.label|default('N/A') %}
{% set entityType = 'Category' |trans({}, 'config') %}

{% set entityInfo = [
    {'title': 'Created', 'value': entity.created ? entity.created|oro_format_datetime : 'N/A'},
] %}
