{% import 'PimUIBundle:Default:page_elements.html.twig' as elements %}
{% import 'PimDataGridBundle::macros.html.twig' as dataGrid %}

{% block content %}
    <div id="association-type-index">
    </div>
{% endblock %}

{% block head_script %}
    <script>
        require(
            [
                'jquery',
                'pim/form-builder',
                'pim/fetcher-registry'
            ],
            function (
                $,
                FormBuilder,
                FetcherRegistry
            ) {
                FetcherRegistry.initialize().done(function () {
                    FormBuilder.build('pim-association-type-index')
                        .then(function (form) {
                            form.setElement('#association-type-index').render();
                        });
                });
            });
    </script>
{% endblock %}
